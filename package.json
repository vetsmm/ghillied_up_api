  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "deploy": "npm ci && npm install --ignore-scripts puppeteer && npm run prisma:migrate:deploy && npm run build && npm run start:prod",
    "csu:dev": "npx ts-node -r tsconfig-paths/register src/create-super-user.ts ",
    "csu:prod": "node dist/src/create-super-user.js",
    "preload": "node -e \"import('geolite2-redist').then(geolite => geolite.downloadDbs())\""
  },
